{
	"name": "fpsmath",
	"private": true,
	"version": "3.3.4",
	"homepage": "https://fpsmath.xyz",
	"license": "AGPL-3.0-or-later",
	"main": "dist/index.js",
	"bugs": {
		"url": "https://github.com/animafps/fpsmath/issues"
	},
	"author": {
		"name": "Anima",
		"email": "animafps@pm.me"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/animafps/fpsmath.git"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write {src,tests}/**/*.ts",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive",
		"start": "node --enable-source-maps dist/index.js",
		"release": "semantic-release",
		"pretty-quick": "pretty-quick --staged --pattern '{src,tests}/**/*.ts'",
		"postinstall": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^14.1.0",
		"@commitlint/config-conventional": "^14.1.0",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^16.11.9",
		"@types/semantic-release": "^17.2.3",
		"@types/ws": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.0.3",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.2",
		"semantic-release": "^18.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.2"
	},
	"dependencies": {
		"@discordjs/collection": "^0.3.2",
		"@sapphire/decorators": "^3.1.4",
		"@sapphire/discord.js-utilities": "^4.1.3",
		"@sapphire/framework": "^2.1.4",
		"@sapphire/pieces": "3.1.1",
		"@sapphire/plugin-api": "^3.0.2",
		"@sapphire/plugin-logger": "^2.0.2",
		"@sapphire/utilities": "^3.1.0",
		"@sentry/node": "^6.15.0",
		"discord-api-types": "^0.24.0",
		"discord.js": "^13.3.1",
		"dotenv": "^10.0.0"
	},
	"engines": {
		"node": "16.x"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/changelog",
				{
					"changelogFile": "docs/changelog.md"
				}
			],
			"@semantic-release/npm",
			[
				"@semantic-release/git",
				{
					"assets": [
						"docs/changelog.md",
						"package.json"
					],
					"message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
				}
			],
			"@semantic-release/github"
		]
	},
	"packageManager": "yarn@3.1.0"
}
