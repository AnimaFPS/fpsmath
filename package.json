{
	"name": "fpsmath",
	"private": true,
	"version": "3.4.0",
	"homepage": "https://fpsmath.xyz",
	"license": "AGPL-3.0-or-later",
	"main": "dist/index.js",
	"bugs": {
		"url": "https://github.com/animafps/fpsmath/issues"
	},
	"author": {
		"name": "Anima",
		"email": "animafps@pm.me"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/animafps/fpsmath.git"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write {src,tests}/**/*.ts",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive",
		"start": "node --enable-source-maps dist/index.js",
		"release": "standard-version",
		"pretty-quick": "pretty-quick --staged --pattern '{src,tests}/**/*.ts'",
		"postinstall": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"@types/node": "^16.11.12",
		"@types/ws": "^8.2.2",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^5.6.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.4.1",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.2",
		"standard-version": "^9.3.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.3"
	},
	"dependencies": {
		"@discordjs/collection": "^0.3.2",
		"@sapphire/decorators": "^3.1.4",
		"@sapphire/discord.js-utilities": "^4.1.5",
		"@sapphire/framework": "^2.2.1",
		"@sapphire/pieces": "3.2.0",
		"@sapphire/plugin-api": "^3.1.1",
		"@sapphire/plugin-logger": "^2.1.1",
		"@sapphire/utilities": "^3.1.0",
		"@sentry/node": "^6.16.1",
		"discord-api-types": "^0.25.2",
		"discord.js": "^13.3.1",
		"dotenv": "^10.0.0"
	},
	"engines": {
		"node": "16.x"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"packageManager": "yarn@3.1.1"
}
